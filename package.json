    "semver": "^7.7.1",
    "turndown": "^7.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^20.17.23",
    "@types/pg": "~8.11.11",
    "@types/semver": "^7.5.8",
